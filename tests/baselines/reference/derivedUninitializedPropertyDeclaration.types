=== tests/cases/conformance/classes/propertyMemberDeclarations/derivedUninitializedPropertyDeclaration.ts ===
class A {
>A : A

    property = 'x';
>property : string
>'x' : "x"
}
class B extends A {
>B : B
>A : A

    property; // error
>property : any
}
class BD extends A {
>BD : BD
>A : A

    declare property; // still has implicit any
>property : any
}
class C {
>C : C

    p: string;
>p : string
}
class D extends C {
>D : D
>C : C

    p: 'hi'; // error
>p : "hi"
}
class DD extends C {
>DD : DD
>C : C

    declare p: 'bye'; // ok
>p : "bye"
}

